<script>

$(async function()
{

var page_container = $('#centerEmployeesPermissionsContainer');
if ( !page_container[0] )
    return;

var clinicSelect = page_container.find('#clinicSelect');
var employeeTypesSelect = page_container.find('#employeeTypesSelect');

var permissions_accordion1 = page_container.find('#permissions_accordion1');
var submit_btn = page_container.find('#submit_btn');
var submit_for_each_btn = page_container.find('#submit_for_each_btn');

submit_btn.on('click', async e =>
{
    SectionLoader(permissions_accordion1);

    var response = await EMPLOYEE_MODEL.type_permission_setBatch({
        administration_id: clinicSelect.find(':selected').val(),
        employee_type_id: employeeTypesSelect.find(':selected').val(),
        permissions: checkedPerms()
    });

    SectionLoader(permissions_accordion1, '');
    if ( response.code == 404 )
    {
        if ( FUI_DISPLAY_LANG.lang == 'ar' )
            CreateToast('اشعار', response.message, 'الآن');
        else if ( FUI_DISPLAY_LANG.lang == 'fr' )
            CreateToast('Notification', response.message, 'maintenant');
        return;
    }

    if ( FUI_DISPLAY_LANG.lang == 'ar' )
        CreateToast('اشعار', response.message, 'الآن');
    else if ( FUI_DISPLAY_LANG.lang == 'fr' )
        CreateToast('Notification', response.message, 'maintenant');
});
//
submit_for_each_btn.on('click', async e =>
{
    SectionLoader(permissions_accordion1);

    var response = await EMPLOYEE_MODEL.type_permission_setBatch({
        administration_id: 'all',
        employee_type_id: employeeTypesSelect.find(':selected').val(),
        permissions: checkedPerms()
    });

    SectionLoader(permissions_accordion1, '');
    if ( response.code == 404 )
    {
        if ( FUI_DISPLAY_LANG.lang == 'ar' )
            CreateToast('اشعار', response.message, 'الآن');
        else if ( FUI_DISPLAY_LANG.lang == 'fr' )
            CreateToast('Notification', response.message, 'maintenant');
        return;
    }

    if ( FUI_DISPLAY_LANG.lang == 'ar' )
        CreateToast('اشعار', response.message, 'الآن');
    else if ( FUI_DISPLAY_LANG.lang == 'fr' )
        CreateToast('Notification', response.message, 'maintenant');
});

SectionLoader(clinicSelect.parent());
var clinics = await CLINIC_MODEL.search('');
SectionLoader(clinicSelect.parent(), '');
if ( clinics.code == 200 )
{
    var html = '';
    clinics.data.forEach(clinic =>
    {
        html += `<option value="${clinic.clinicId}">${clinic.clinicName}</option>`;
    });
    clinicSelect.html(html);
    clinicSelect.trigger('change');
}

SectionLoader(employeeTypesSelect.parent());
employeeTypesSelect.html('');
var employee_type = await EMPLOYEE_MODEL.types_index();
SectionLoader(employeeTypesSelect.parent(), '');
if ( employee_type.code == 200 )
{
    var html = '';
    employee_type.data.forEach(type =>
    {
        html += `<option value="${type.employee_type_id}" data-code="${type.employee_type_code}">${FUI_DISPLAY_LANG.views.pages.global.employee_type_codes[type.employee_type_code]}</option>`;
    });
    employeeTypesSelect.html(html);
    selectEmployeeTypePermissions();
}
// select administration
clinicSelect.off('change');
clinicSelect.on('change', e =>
{
    selectEmployeeTypePermissions();
});
// select employee type
employeeTypesSelect.off('change');
employeeTypesSelect.on('change', e =>
{
    var selected = employeeTypesSelect.find(':selected')

    submit_for_each_btn.text(`${FUI_DISPLAY_LANG.views.pages.global.save_changes_for_each_attribute_button}: ${selected.text()}`)
    selectEmployeeTypePermissions();
});
employeeTypesSelect.trigger('change')
// checked permissions
function checkedPerms()
{
    var granted = [];
    var denied = [];
    var checks = permissions_accordion1.find('[data-role="permission"]');
    for (let i = 0; i < checks.length; i++) 
    {
        const check = $(checks[i]);
        if ( check.is(':checked') )
            granted.push(check.attr('id'));
        else 
            denied.push(check.attr('id'));
    }

    return {
        granted: granted,
        denied: denied,
    }
}
// select employee type permissions
async function selectEmployeeTypePermissions()
{
    var response = await EMPLOYEE_MODEL.type_permission_index({
        administration_id: clinicSelect.find(':selected').val(),
        employee_type_id: employeeTypesSelect.find(':selected').val()
    });
    if ( response.code == 404 )
        return;
    
    var permissions = response.data;
    var checks = permissions_accordion1.find('[data-role="permission"]');
    for (let i = 0; i < checks.length; i++) 
    {
        const check = $(checks[i]);
        // granted
        for (let j = 0; j < permissions.granted.length; j++) 
        {
            const perm = permissions.granted[j];
            if ( check.attr('id') == perm )
                check.prop('checked', true);
        }
        // denied
        for (let j = 0; j < permissions.denied.length; j++) 
        {
            const perm = permissions.denied[j];
            if ( check.attr('id') == perm )
                check.prop('checked', false);
        }
    }
}

})

</script>